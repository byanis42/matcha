# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:8000
VITE_WEBSOCKET_PATH=/socket.io

# Application Configuration
VITE_APP_NAME=Matcha
VITE_APP_DESCRIPTION=Dating App with Clean Architecture
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=development

# Features
VITE_ENABLE_MAPS=true
VITE_ENABLE_SOCIAL_AUTH=true
VITE_ENABLE_NOTIFICATIONS=true

# External Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_FACEBOOK_APP_ID=your-facebook-app-id

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_PICTURES=5
VITE_ALLOWED_EXTENSIONS=jpg,jpeg,png,gif,webp

# Geolocation
VITE_DEFAULT_LATITUDE=48.8566
VITE_DEFAULT_LONGITUDE=2.3522
VITE_DEFAULT_CITY=Paris
VITE_DEFAULT_COUNTRY=France